    <formulas>
        <name>varF_AccountLink</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.RecordType.DeveloperName} == &apos;Individual_Partner&apos;,LINKTO({!varF_PersonAccountName},  LEFT({!$Api.Partner_Server_URL_260}, FIND(&apos;/services&apos;,{!$Api.Partner_Server_URL_260}))&amp;{!$Record.Id}) , LINKTO({!$Record.Name},  LEFT({!$Api.Partner_Server_URL_260}, FIND(&apos;/services&apos;,{!$Api.Partner_Server_URL_260}))&amp;{!$Record.Id}))</expression>
    </formulas>
    <formulas>
        <name>varF_AccountName</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.RecordType.DeveloperName} == &apos;Individual_Partner&apos;, {!varF_PersonAccountName}, {!$Record.Name})</expression>
    </formulas>
    <formulas>
        <name>varF_AccountRecordPageLink</name>
        <dataType>String</dataType>
        <expression>{!$Label.Account_Record_Page_Base_Url}&amp;{!$Record.Id}&amp;&apos;/view&apos;</expression>
    </formulas>
    <formulas>
        <name>varF_ContactLink</name>
        <dataType>String</dataType>
        <expression>LINKTO({!Loop_Over_Contact_Records.Name},  LEFT({!$Api.Partner_Server_URL_260}, FIND(&apos;/services&apos;,{!$Api.Partner_Server_URL_260}))&amp;{!Loop_Over_Contact_Records.Id}) + &apos;&lt;Br/&gt;&apos;</expression>
    </formulas>
    <formulas>
        <name>varF_PersonAccountName</name>
        <dataType>String</dataType>
        <expression>{!$Record.FirstName} &amp; &apos; &apos; &amp; {!$Record.LastName}</expression>
